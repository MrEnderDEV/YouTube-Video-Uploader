            description="Compile module videouploader"/>

    <target name="compile.module.videouploader.production" depends="register.custom.compilers"
            description="Compile module videouploader; production classes"/>

    <target name="compile.module.videouploader.tests"
            depends="register.custom.compilers,compile.module.videouploader.production"
            description="compile module videouploader; test classes" unless="skip.tests"/>

    <target name="clean.module.videouploader" description="cleanup module">
        <delete dir="${videouploader.output.dir}"/>
        <delete dir="${videouploader.testoutput.dir}"/>
    </target>


